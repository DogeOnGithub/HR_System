
@{
    ViewBag.Title = "AddSalaryItem";
}

<h1 class="text-center">AddSalaryItem</h1>
<br />
<br />
<br />
<br />
<div class="container col-lg-6" style="float:none; display:block; margin-left:auto; margin-right:auto">
    <form method="post" action="/SalaryItemSettings/SaveSalaryItem">
        <div class="form-group input-group-lg">
            <label for="ItemName">职称</label>
            <input type="text" class="form-control" id="ItemName" placeholder="ItemName" name="ItemName" required>
        </div>
        <button type="submit" class="btn btn-primary btn-block input-group-lg">Submit</button>
    </form>
</div>


